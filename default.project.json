{
  "name": "template",
  "globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "template": {
        "$path": "include",

        "node_modules": {
          "$className": "Folder",

          "@rbxts": {
            "$path": "node_modules/@rbxts"
          },

          "@grand-hawk": {
            "$path": "node_modules/@grand-hawk"
          }
        }
      }
    },

    "StarterGui": {
      "template": {
        "$path": "dist"
      }
    }
  }
}
